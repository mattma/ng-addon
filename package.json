{
  "name": "ng-addon",
  "description": "An Angular command line utility (ngg) to generate addon packages",
  "version": "0.1.0",
  "homepage": "https://github.com/mattma/ng-addon",
  "author": {
    "name": "Matt Ma",
    "email": "matt@mattmadesign.com",
    "url": "http://mattmadesign.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mattma/ng-addon.git"
  },
  "bugs": {
    "url": "https://github.com/mattma/ng-addon/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mattma/ng-addon/blob/master/LICENSE.md"
    }
  ],
  "main": "src/commands/cli",
  "preferGlobal": "true",
  "bin": {
    "ngg": "./bin/ngg"
  },
  "engines": {
    "node": ">= 0.10.0",
    "iojs": ">= 1.2.0"
  },
  "engineStrict": true,
